// 2d matrix sum
#include <stdio.h>
int main() 
{ 
    int i,j,r,c;
    scanf("%d %d", &r, &c);
    int mat[r][c];
    int mat2[r][c];
    int mat3[r][c];
    for(i=0; i<r;i++)
    {
        for(j=0;j<c;j++)
        { scanf("%d", &mat[i][j]); }
    }
    for(i=0; i<r;i++)
    {
        for(j=0;j<c;j++)
        { printf("%d", &mat[i][j]); }
    }
    for(i=0; i<r;i++)
    {
        for(j=0;j<c;j++)
        { scanf("%d", &mat2[i][j]); }
    }
    for(i=0; i<r;i++)
    {
        for(j=0;j<c;j++)
        { printf("%d", &mat2[i][j]); }
    }
    for(i=0; i<r;i++)
    {
        for(j=0;j<c;j++)
        { 
            mat3[i][j]=mat[i][j]+mat2[i][j];
            printf("%d", &mat3[i][j]); 
        }
    }
    
    return 0;
}
