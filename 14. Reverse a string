// Reverse a string.
#include<stdio.h>
#include<string.h>
int main ()
{
    char a[100],temp;
    scanf("%s", &a);
    int size, i=0,j=0;
    size= strlen(a);
    j= size-1;
    while(i<j)
    {
        temp= a[j];
        a[j]=a[i];
        a[i]= temp;
        i++;
        j--;
    }
    printf("%s", a);

}
