// 11. Take a character array of size 20 (user input) and convert all letters to the uppercase letter.
#include<stdio.h>
#include<string.h>

int main ()
{
    int n;
    scanf("%d",&n);
    char s[n];
    scanf("%s",&s);
    int sz = strlen(s);
    for(int i=0;i<sz;i++){
        if(s[i]>='a' && s[i]<='z'){
            s[i] = s[i] - 32;
        }
    }
    printf("%s",s);
    return 0;
}
