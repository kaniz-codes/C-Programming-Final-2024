// Convert all letters to uppercase letter in array.
#include<stdio.h>
#include<string.h>
int main()
{
    char a[20];
    int i, s;
    scanf("%s",a);
    s= strlen(a);
    for(i=0; i<=s; i++)
    {
        if(a[i]>='a' && a[i]<='z')
        {
            a[i] = a[i] -32;
        }
    }
    printf("%s", a);
}

